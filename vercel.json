{
  "version": 2,
  "name": "quantumcoin-ui",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.html)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.js)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "pages/api/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_BTC_ADDRESS": "@next_public_btc_address",
    "EXCHANGE_AVAILABLE_FLOAT": "@exchange_available_float",
    "QTC_USD_PRICE": "@qtc_usd_price",
    "NEXT_PUBLIC_REVSTOP_DEFAULT_ON": "@next_public_revstop_default_on"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}
