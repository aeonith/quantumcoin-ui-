{
  "version": 2,
  "name": "quantumcoin-mainnet-explorer",
  "builds": [
    {
      "src": "AEONITH_GRADE_BACKEND.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/status",
      "dest": "/AEONITH_GRADE_BACKEND.js"
    },
    {
      "src": "/explorer/(.*)",
      "dest": "/AEONITH_GRADE_BACKEND.js"
    },
    {
      "src": "/health",
      "dest": "/AEONITH_GRADE_BACKEND.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_BASE": "https://quantumcoin-ui.vercel.app",
    "NEXT_PUBLIC_EXPLORER_URL": "https://quantumcoin-ui.vercel.app"
  },
  "functions": {
    "AEONITH_GRADE_BACKEND.js": {
      "maxDuration": 30
    }
  }
}
